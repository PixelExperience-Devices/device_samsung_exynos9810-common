    chown system system /proc/pwrsrc
    chmod 0440 /proc/pwrsrc
    chown system system /proc/extra
    chmod 0440 /proc/extra

    chmod 0660 /sys/power/wakeup_count

    # vibrator
    chown system system /sys/class/timed_output/vibrator/intensity
